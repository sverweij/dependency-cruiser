strict digraph "dependency-cruiser output"{
    {{#if graphAttrs}}{{{graphAttrs}}}{{/if}}
    {{#if nodeAttrs}}node [{{{nodeAttrs}}}]{{/if}}
    {{#if edgeAttrs}}edge [{{{edgeAttrs}}}]{{/if}}

{{#modules}}
    {{#if folder}}
    {{#each path}}subgraph "cluster_{{{aggregateSnippet}}}" {label="{{{snippet}}}" {{/each~}}
        "{{{source}}}" [label="{{{label}}}" {{#if rule}}tooltip="{{{rule.name}}}" {{/if}}{{#if URL}}URL="{{{URL}}}" {{/if}}{{#if themeAttrs}}{{{themeAttrs}}}{{/if}}]{{#each path}} }{{/each}}
    {{else}}
    "{{{source}}}" [label="{{{label}}}" {{#if rule}}tooltip="{{{rule.name}}}" {{/if}}{{#if URL}}URL="{{{URL}}}" {{/if}}{{#if themeAttrs}}{{{themeAttrs}}}{{/if}}]
    {{/if}}
{{/modules}}

{{#modules}}
    {{#dependencies}}
    "{{{../source}}}" -> "{{{resolved}}}"{{#if hasExtraAttributes}} [{{#if rule.name}}xlabel="{{{rule.name}}}" tooltip="{{{rule.name}}}" {{/if}}{{#if themeAttrs}}{{{themeAttrs}}}{{/if}}]{{/if}}
    {{/dependencies}}
{{/modules}}
}
